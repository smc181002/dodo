{
  "manifest_version": 3,
  "name": "Dodo",
  "short_name": "dodo",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "192": "android-chrome-192x192.png",
    "512": "android-chrome-512x512.png" 
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}
